<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for stock.line -->
        <record id="seq_stock_line" model="ir.sequence">
            <field name="name">Stock Line</field>
            <field name="code">stock.line</field>
            <field name="prefix">SL</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
   	<data noupdate="1">
        <!-- Sequences for heat.heat -->
        <record id="seq_heat_heat" model="ir.sequence">
            <field name="name">Heat</field>
            <field name="code">heat.heat</field>
            <field name="prefix">H</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>   	
    </data>
    <data noupdate = "1">
        <!-- Sequences for test.test -->
        <record id="seq_test_test" model="ir.sequence">
            <field name="name">Lab Test Records</field>
            <field name="code">test.test</field>
            <field name="prefix">T/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>        
        
        <!-- Sequences for test.line -->
        <record id="seq_test_line" model="ir.sequence">
            <field name="name">Lab Test Lines</field>
            <field name="code">test.line</field>
            <field name="prefix">TL/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>                        
    </data>
</odoo>
