<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for mill.production -->
        <record id="seq_mill_production" model="ir.sequence">
            <field name="name">Mill Production</field>
            <field name="code">mill.production</field>
            <field name="prefix">MP</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for mill.production.line -->
        <record id="seq_mill_production_line" model="ir.sequence">
            <field name="name">Mill Production Line</field>
            <field name="code">stock.line.production</field>
            <field name="prefix">MPL</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>        

    </data>
</odoo>
